{
  "name": "license-subscription-system",
  "version": "1.0.0",
  "description": "License Short-term Subscription System",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install && cd ../api && npm install",
    "build": "cd client && npm run build",
    "vercel-build": "npm install && cd client && npm install && cd ../api && npm install && cd ../client && npm run build"
  },
  "keywords": ["license", "subscription", "management"],
  "author": "License Subscription System Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}